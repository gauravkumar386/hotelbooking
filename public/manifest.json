{
  "short_name": "Hotel Booking",
  "name": "Hotel Booking App",
  "description": "A simple hotel booking application",
  "lang": "en-US",
  "dir": "ltr",
  "scope": "/",
  "orientation": "portrait",
  "display_override": [
    "window-controls-overlay",
    "minimal-ui"
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "categories": [
    "travel",
    "hotel",
    "booking",
    "reservation"
  ],
  "screenshots": [
    {
      "src": "screenshot1.png",
      "type": "image/png",
      "sizes": "1280x720",
      "label": "Home Screen"
    },
    {
      "src": "screenshot2.png",
      "type": "image/png",
      "sizes": "1280x720",
      "label": "Booking Details"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "name",
      "text": "description",
      "url": "url"
    }
  },
  "icons": [
    {
      "src": "icon-192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "icon-512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
